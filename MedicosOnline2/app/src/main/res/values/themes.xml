<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.MedicosOnline" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/blue_2cb</item>
        <item name="colorPrimaryVariant">@color/blue_light</item>
        <item name="colorOnPrimary">@color/text_colort</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/blue_2cb</item>
        <item name="colorSecondaryVariant">@color/blue_light</item>
        <item name="colorOnSecondary">@color/text_colort</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.MedicosOnline.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.MedicosOnline.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.MedicosOnline.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>